class employ:
  def __init__(self,name,age):
     self.name = name
     self.age = age
  def show(self):
    print(f"your name is {self.name} and your age is {self.age} ")
  @classmethod  
  def new(cls,str):
    a, b= str.split(",")
    return cls(a,b)
str = "kabber,23" 
a = employ.new(str)
a.show()


